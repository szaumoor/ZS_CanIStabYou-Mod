COPY_EXISTING_REGEXP ~.*\.CRE~ ~override~
    SET dragon = IDS_OF_SYMBOL(race DRAGON)
    READ_BYTE 0x272 race
    PATCH_IF race = dragon
        AND "%SOURCE_RES%" STRING_COMPARE_CASE "DRAGBLUE" != 0
        AND "%SOURCE_RES%" STRING_COMPARE_CASE "ABAZIGAL" != 0
    BEGIN
      LPF DELETE_EFFECT INT_VAR match_opcode = 292 END
    END
BUT_ONLY
